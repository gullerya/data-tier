{
  "env": {
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "script"
  },
  "extends": "eslint:recommended",
  "rules": {
    "no-console": "off"
  },
  "globals": {
    "Map": true,
    "WeakMap": true,
    "Symbol": true,
    "Reflect": true
  }
}